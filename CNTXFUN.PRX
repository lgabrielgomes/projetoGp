							"CNE_ITEM",;
							"CNE_VLTOT";
						}
		EndIf
	Endif
ElseIf  cAlias == "CNA"
		If aRetCab <> nil
			aRetCab := {	"CNA_CONTRA",;
							"CNA_REVISA",;
							"CNA_NUMERO",;
							"CNA_FORNEC",;
							"CNA_LJFORN",;
							"CNA_CLIENT",;
							"CNA_LOJACL";
						}
		EndIf
		If aRetIt <> nil
