ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß?
/*/
Function CntQtdVend(cTab)

Local cCnt     := "1"
Local aStruct  := ""
Local lContinua:= .T.
Local nVend    := 0

Default cTab := "SE1"

nVend := 0

Do Case

	Case cTab == "SE1"
		aStruct := SE1->(dbStruct())
		While ( lContinua )
			If (   aScan(aStruct,{|x| Trim(x[1]) == "E1_VEND"+cCnt})  != 0 ) .AND. ;
			     ( aScan(aStruct,{|x| Trim(x[1]) == "E1_COMIS"+cCnt}) != 0 ) .AND. ;
			     ( aScan(aStruct,{|x| Trim(x[1]) == "E1_VALCOM"+cCnt})!= 0 )
				nVend++
				cCnt := Soma1(cCnt,1)
			Else
				lContinua := .F.
			EndIf
		EndDo

	Case cTab == "SC5"
		aStruct := SC5->(dbStruct())
		While ( lContinua )
			If (   aScan(aStruct,{|x| Trim(x[1]) == "C5_VEND"+cCnt})  != 0 ) .AND. ;
			     ( aScan(aStruct,{|x| Trim(x[1]) == "C5_COMIS"+cCnt}) != 0 )
				nVend++
				cCnt := Soma1(cCnt,1)
			Else
				lContinua := .F.
			EndIf
		EndDo

	Case cTab == "SC6"
		aStruct := SC6->(dbStruct())
		While ( lContinua )
			If ( aScan(aStruct,{|x| Trim(x[1]) == "C6_COMIS"+cCnt}) != 0 )
				nVend++
				cCnt := Soma1(cCnt,1)
			Else
				lContinua := .F.
			EndIf
		EndDo

EndCase

nVend := Max(nVend,5)

Return(nVend)

