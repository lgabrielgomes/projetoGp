/*/{Protheus.doc} AlcDifEC(oModel,cNumDoc,aHeadCNB,aColsCNB)
Gera alçada por entidade contabil somente para itens alterados

Parametros:
oModel		Modelo ativo.
cNumDoc	Numero do documento.
aHeadCNB	Cabeçalho 	CNB	(Itens da Planilha)
aColsCNB	Colunas 	CNB	(Itens da Planilha)
aHeadCNZ	Cabeçalho	CNZ	(Rateio)
aColsCNZ	Colunas	    CNZ	(Rateio)

@author Israel Escorizza
@since 09/06/2015
@version 1.0
@return lRet

/*/
//-------------------------------------------------------------------
Function AlcDifEC(oModel,cNumDoc,aHeadCNB,aColsCNB,aHeadCNZ,aColsCNZ)
Local lRet			:= .T.
Local lMantem		:= .F.

Local oModelCNS 	:= oModel:GetModel("CNSDETAIL")
Local oModelCNB 	:= oModel:GetModel("CNBDETAIL")

Local aChangeCNF 	:= {}
Local aChangeCNS 	:= {}
Local aChangeCNB 	:= {}

Local cChaveB		:= ""
Local cChaveZ		:= ""

Local nC			:= 1
Local nD			:= 0

//- Ajusta tamanho da variavel para ser compativel com tamanho do documento de aprovação.
cNumDoc := Left(cNumDoc,TamSX3('CN9_NUMERO')[1]+TamSX3('CN9_REVISA')[1])

//- Verifica se foi realizada alteração no cronograma financeiro (CNF)
cSeek := {|| xFilial("CNF")+cDoc+cRevVig+oDetail:GetValue("CNF_NUMERO")+oDetail:GetValue("CNF_PARCEL")}
aChangeCNF := GCTAltEC(oModel,'CNF','CNFDETAIL',Left(cNumDoc,Len(CNB->CNB_CONTRA)),,3,cSeek,{"CNF_REVISA"})
If	Len(aChangeCNF)
	If MaEntCtb("CNB","CNZ",cNumDoc,"IR",aHeadCNB,aColsCNB,aHeadCNZ,aColsCNZ,1,dDataBase)
		lRet := .F.
	EndIf
Else
	//- Obtem alterações no cronograma fisico (CNS) e nos itens (CNB).
	cSeek := {|| xFilial("CNS")+cDoc+cRevVig+oDetail:GetValue("CNS_CRONOG")+oDetail:GetValue("CNS_PARCEL")+oDetail:GetValue("CNS_ITEM")}
	aChangeCNS := GCTAltEC(oModel,'CNS','CNSDETAIL',Left(cNumDoc,Len(CNB->CNB_CONTRA)),,1,cSeek,{"CNS_REVISA"})
	cSeek := {|| xFilial("CNB")+cDoc+cRevVig+oDetail:GetModel():GetValue("CNADETAIL", "CNA_NUMERO")+oDetail:GetValue("CNB_ITEM")}
	aChangeCNB := GCTAltEC(oModel,'CNB','CNBDETAIL',Left(cNumDoc,Len(CNB->CNB_CONTRA)),,1,cSeek,{"CNB_REVISA","CNB_PRCORI","CNB_QTDORI"})

	//- Monta Posicionamento dos campos chave no aHeadCNB
	While nC <= Len(aColsCNB)
		lMantem := .F.
		cChaveB := aColsCNB[nC][aScan(aHeadCNB, {|x| Alltrim(x[2]) == 'CNB_CONTRA'})]
		cChaveB += aColsCNB[nC][aScan(aHeadCNB, {|x| Alltrim(x[2]) == 'CNB_REVISA'})]
		cChaveB += aColsCNB[nC][aScan(aHeadCNB, {|x| Alltrim(x[2]) == 'CNB_NUMERO'})]
		cChaveB += aColsCNB[nC][aScan(aHeadCNB, {|x| Alltrim(x[2]) == 'CNB_ITEM'})]
		For nD := 1 to Len(aChangeCNS)
			oModelCNS:GoLine(aChangeCNS[nD][1])
			If cChaveB == oModelCNS:GetValue("CNS_CONTRA")+oModelCNS:GetValue("CNS_REVISA")+oModelCNS:GetValue("CNS_PLANI")+oModelCNS:GetValue("CNS_ITEM")
				lMantem := .T.
			EndIf
		Next nD
		If !lMantem
			For nD := 1 to Len(aChangeCNB)
				oModelCNB:GoLine(aChangeCNB[nD][1])
				If cChaveB == oModelCNB:GetValue("CNB_CONTRA")+CnGetRevAt(oModelCNB:GetValue("CNB_CONTRA"))+oModelCNB:GetValue("CNB_NUMERO")+oModelCNB:GetValue("CNB_ITEM") .OR. oModelCNB:IsInserted()
					lMantem := .T.
				EndIf
			Next nD
		EndIf
		If !lMantem
			aDel(aColsCNB,nC)
			aSize(aColsCNB,Len(aColsCNB)-1)
		Else
			nC++
		EndIf
	EndDo

	nC:= 1
	While nC <= Len(aColsCNZ)
		lMantem := .F.
		cChaveZ := aColsCNZ[nC][aScan(aHeadCNZ, {|x| Alltrim(x[2]) == 'CNZ_CONTRA'})]
		cChaveZ += aColsCNZ[nC][aScan(aHeadCNZ, {|x| Alltrim(x[2]) == 'CNZ_REVISA'})]
		cChaveZ += aColsCNZ[nC][aScan(aHeadCNZ, {|x| Alltrim(x[2]) == 'CNZ_CODPLA'})]
		cChaveZ += aColsCNZ[nC][aScan(aHeadCNZ, {|x| Alltrim(x[2]) == 'CNZ_ITCONT'})]
		For nD := 1 To Len(aColsCNB)
			cChaveB := aColsCNB[nD][aScan(aHeadCNB, {|x| Alltrim(x[2]) == 'CNB_CONTRA'})]
			cChaveB += aColsCNB[nD][aScan(aHeadCNB, {|x| Alltrim(x[2]) == 'CNB_REVISA'})]
			cChaveB += aColsCNB[nD][aScan(aHeadCNB, {|x| Alltrim(x[2]) == 'CNB_NUMERO'})]
			cChaveB += aColsCNB[nD][aScan(aHeadCNB, {|x| Alltrim(x[2]) == 'CNB_ITEM'})]

			If cChaveB == cChaveZ
				lMantem := .T.
			EndIf
		Next nD
		If !lMantem
			aDel(aColsCNZ,nC)
			aSize(aColsCNZ,Len(aColsCNZ)-1)
		Else
			nC++
		EndIf
	EndDo

	If MaEntCtb("CNB","CNZ",cNumDoc,"IR",aHeadCNB,aColsCNB,AHeadCNZ,aColsCNZ,1,dDataBase)
		lRet := .F.
	EndIf
EndIf

Return lRet

