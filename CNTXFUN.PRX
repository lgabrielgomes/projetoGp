/*/
//-------------------------------------------------------------------
Function CnTpProd(cRotina, cPlan, cContra, cRev, nLinha)
Local aArea	:= GetArea()
Local aSaveLines	:= {}
Local cAliasCNB	:= ""
Local cFilSB5	:= xFilial("SB5")
Local lServ := .F.
Local lProd := .F.
Local nRet	:= 3
Local nX	:= 0
Local oModel	:= Nil
