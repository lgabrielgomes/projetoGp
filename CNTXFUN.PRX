	cEspCtr := Iif(Cn300RetSt('COMPRA'),'1','2')
ElseIf !IsInCallStack("CNTA300RUS") .And.  (!IsInCallStack("CNTA300").and. cPaisLoc!="RUS") .Or. IsInCallStack("CN100CtrRet") .Or. IsInCallStack("CN100CtrAdia") 
	cEspCtr := CN9->CN9_ESPCTR
