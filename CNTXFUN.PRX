/*/{Protheus.doc} CnDecimais()
Verifica a compatibilidade de casas decimais com os módulos
Faturamento e Compras

@author jose.delmondes
@since 08/05/2017
@version 1.0
@return
/*/
//-------------------------------------------------------------------
Function CnDecimais()
Local nDecQuant	:= TamSx3("CNE_QUANT")[2]
Local nDecUnit	:= TamSX3("CNE_VLUNIT")[2]
Local nDecTot	:= TamSX3("CNE_VLTOT")[2]
Local nDecDesc	:= TamSX3("CNE_VLDESC")[2]
Local cTexto	:= STR0068 + ' ' + STR0069 + ' ' + STR0070
Local lMsg		:= .F.

// Verifica divergência entre os campos de quantidade
If (TamSX3("CNE_QTDSOL")[2] <> nDecQuant) .Or. (TamSX3("CNE_QTAMED")[2] <> nDecQuant) .Or. (TamSX3("C6_QTDVEN")[2] <> nDecQuant) .Or. (TamSX3("C7_QUANT")[2] <> nDecQuant);
	   .Or. (TamSX3("C7_QTDSOL")[2] <> nDecQuant) .Or. (TamSX3("CNB_QUANT")[2] <> nDecQuant) .Or. (TamSX3("CNB_QTDMED")[2] <> nDecQuant) .Or. (TamSX3("C1_QUANT")[2] <> nDecQuant)  
	cTexto += Chr(13)+Chr(10)+Chr(13)+Chr(10)+STR0071
	lMsg := .T.
EndIf

// Verifica divergência entre os campos de valor unitário
If (TamSX3("CNE_VUNORI")[2] <> nDecUnit) .Or. (TamSX3("C6_PRUNIT")[2] <> nDecUnit) .Or. (TamSX3("C6_PRCVEN")[2] <> nDecUnit) .Or. (TamSX3("C7_PRECO")[2] <> nDecUnit);
		.Or. (TamSX3("CNB_VLUNIT")[2] <> nDecUnit) .Or. (TamSX3("C1_VUNIT")[2] <> nDecUnit) .Or. (TamSX3("C1_PRECO")[2] <> nDecUnit)
	cTexto += Chr(13)+Chr(10)+Chr(13)+Chr(10)+STR0072
	lMsg := .T.
EndIf

// Verifica divergência entre os campos de valor total
If (TamSX3("CND_VLTOT")[2] <> nDecTot) .Or. (TamSX3("C6_VALOR")[2] <> nDecTot) .Or. (TamSX3("C7_TOTAL")[2] <> nDecTot) .Or. (TamSX3("CNB_VLTOT")[2] <> nDecTot);
	 .Or. (TamSX3("CXN_VLTOT")[2] <> nDecTot) .Or. (TamSX3("C1_TOTAL")[2] <> nDecTot)
	cTexto += Chr(13)+Chr(10)+Chr(13)+Chr(10)+STR0073
	lMsg := .T.
EndIf

// Verifica divergência entre os campos de desconto
If (TamSX3("C6_VALDESC")[2] <> nDecDesc) .Or. (TamSX3("C7_VLDESC")[2] <> nDecDesc) .Or. (TamSX3("CNB_VLDESC")[2] <> nDecDesc) .Or. (TamSX3("CXN_VLDESC")[2] <> nDecDesc)
	cTexto += Chr(13)+Chr(10)+Chr(13)+Chr(10)+STR0074
	lMsg := .T.
EndIf

If lMsg
	cTexto += Chr(13)+Chr(10)+Chr(13)+Chr(10)+STR0083
	MsgAlert(cTexto, STR0051+"!")
EndIf

Return

