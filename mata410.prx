	aYesFields := {}
	DbSelectArea("SX3")
	SX3->(DbSetOrder(1))	//X3_ARQUIVO + X3_ORDEM
	SX3->(DbSeek( "SC6" ))
	While ( SX3->(!Eof()) .And. SX3->X3_ARQUIVO == "SC6" )
		aAdd( aYesFields, AllTrim(SX3->X3_CAMPO) )
