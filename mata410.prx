If lContinua
	dbSelectArea("SC5")
	IF !GetMV("MV_EEC0023",,.F.) .AND. !Empty(SC5->C5_PEDEXP) .And. nModulo != 29 .And. ( Type("l410Auto") == "U" .OR. !l410Auto )
		Help(" ",1,"MTA410ALT")
		lContinua := .F.
	EndIf
